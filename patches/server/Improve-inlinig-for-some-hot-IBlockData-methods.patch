@@ -0,0 +0,0 @@ public abstract class BlockBehaviour {
             }
         }
 
-        public boolean canOcclude() {
+        public final boolean canOcclude() { // Paper
             return this.canOcclude;
         }
 
@@ -0,0 +0,0 @@ public abstract class BlockBehaviour {
             return this.getBlock() == block;
